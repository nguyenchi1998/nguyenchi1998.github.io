{
  "name": "chat",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "dev": "NODE_ENV=development npm run watch bin/www",
    "prod": "npm run build && NODE_ENV=production nodemon ./dist/bin/www",
    "build": "npm run clean && babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "watch": "nodemon --config ./nodemon.json --"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bigint-hash": "^0.2.2",
    "bluebird": "latest",
    "celebrate": "^10.0.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "http-status": "^1.4.0",
    "jade": "^1.11.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongodb": "^3.3.3",
    "mongoose": "^5.7.7",
    "morgan": "~1.9.0",
    "pug": "^2.0.4",
    "rimraf": "^2.7.1",
    "joi": "^14.3.1",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.18.2",
    "firebase-functions": "^3.3.0",
    "nodemon": "^1.19.4"
  }
